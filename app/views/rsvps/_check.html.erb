<% if !rsvp.attended %>
 <%= link_to("#", class: "checkin", data: {id: rsvp.id}, :method => "post", remote: true) do %>
    <%= image_tag(asset_path("Icon/Checkmark/Grey.svg"), class: "check") %>
  <% end %>
<% else %>
  <%= link_to("#", class: "checkout", data: {id: rsvp.id}, :method => "post", remote: true) do %>
    <%= image_tag(asset_path("Icon/Checkmark/Color.svg"), class: "check") %>
  <% end %>
<% end %>
